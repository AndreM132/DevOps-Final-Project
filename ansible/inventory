[managers]
manager ansible_host=manager-vm

[managers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=/home/jenkin/.ssh/id_rsa

[workers]
worker-1 ansible_host=worker-vm 

[workers:vars]
ansible_user=jenkins
ansible_ssh_common_args='-o StrictHostKeyChecking=no'
ansible_ssh_private_key_file=/home/jenkins/.ssh/ansible_id_rsa